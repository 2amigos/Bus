{
    "name": "2amigos/command-bus",
    "type": "library",
    "description": "Simple Command Bus",
    "keywords": [
        "2amigos",
        "Command",
        "Bus"
    ],
    "homepage": "https://github.com/2amigos/command-bus",
    "license": "MIT",
    "authors": [
        {
            "name": "2amigos",
            "email": "hola@2amigos.us",
            "homepage": "http://2amigos.us",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.5.0",
        "psr/log": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "5.*",
        "scrutinizer/ocular": "~1.1",
        "squizlabs/php_codesniffer": "~2.3",
        "fabpot/php-cs-fixer": "@stable",
        "phpmd/phpmd": "@stable"
    },
    "autoload": {
        "psr-4": {
            "Da\\Bus\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Da\\Bus\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
